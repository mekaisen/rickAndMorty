generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model haracter {
  id       Int    @id @default(autoincrement())
  name     String @db.VarChar(255)
  image    String @unique @db.VarChar(255)
  likes    Int    @default(0)
  dislikes Int    @default(0)
}
